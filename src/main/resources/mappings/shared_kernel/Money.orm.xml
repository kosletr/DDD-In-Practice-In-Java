<entity-mappings xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence/orm http://xmlns.jcp.org/xml/ns/persistence/orm_2_2.xsd"
                 version="2.2">

    <embeddable class="com.example.demo.shared_kernel.Money" access="FIELD">
        <attributes>
            <basic name="oneCentCount">
                <column name="oneCentCount"/>
            </basic>
            <basic name="tenCentCount">
                <column name="tenCentCount"/>
            </basic>
            <basic name="quarterCount">
                <column name="quarterCount"/>
            </basic>
            <basic name="oneDollarCount">
                <column name="oneDollarCount"/>
            </basic>
            <basic name="fiveDollarCount">
                <column name="fiveDollarCount"/>
            </basic>
            <basic name="twentyDollarCount">
                <column name="twentyDollarCount"/>
            </basic>
        </attributes>
    </embeddable>
</entity-mappings>